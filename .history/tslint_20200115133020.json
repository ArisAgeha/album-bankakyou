{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {},
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "no-var-requires": false,
    "quotemark": [true, "single"],
    "no-console": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}
